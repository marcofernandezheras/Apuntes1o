Teclalogia:
	ctrl + space     = autocompletar
	ctrl + shift + f = autosangrado
	ctr  + f11       = run
	ctrl + shift + f11 = coverage
	f11				 = debug
	ctrl + 7         = comentar linea

	ctrl + f 		 = buscar / remplazar

	ctrl + alt + h = buscar referencias
	f3			   = ir a implementacion
	f4 (en la clase) = jerarquia de clases

	atl + shift + z = try-catch
Debuging:
	f6  = ejecutar linea actual
	f8  = resumir / continuar
	f5  = entrar a funcion

	ctrl + shift + d = display
	ctrl + shift + i = inspeccionar

